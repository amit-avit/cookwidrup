{
  "name": "cookwidrup",
  "description": "Rupali's food vlog",
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "author": "Amit Kumar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amit-avit/cookwidrup.git"
  },
  "bugs": {
    "url": "https://github.com/amit-avit/cookwidrup/issues"
  },
  "homepage": "https://github.com/amit-avit/cookwidrup#readme",
  "main": "index.js",
  "files": [
    "README.md",
    "css/",
    "dist/",
    "lib-commonjs/",
    "lib/",
    "scss/"
  ],
  "scripts": {
    "test": "run-script eslint && run-script tsc --noEmit && run-script jest",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "clean": "del-cli lib lib-commonjs dist css coverage ../200.html ../index.html ../main.js ../main.js.map --force && jest --clearCache",
    "analyze": "cross-env NODE_ENV=production WEBPACK_ANALYZE=true webpack",
    "build": "run-script clean && run-script build:webapp",
    "build:webapp": "cross-env NODE_ENV=production webpack",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"/__demo__/\" --no-error-on-unmatched-pattern",
    "jest": "cross-env NODE_ENV=test jest --passWithNoTests",
    "prepack": "run-script build",
    "reformat": "run-script eslint --fix --quiet",
    "tsc": "tsc",
    "watch": "webpack --progress --colors --debug --watch",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "..",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    }
  },
  "dependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "core-js": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/generator": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@xornot/run-script": "^0.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.5",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
